# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Database Configuration
DATABASE_URL=your_database_connection_string
DATABASE_HOST=your_database_host
DATABASE_NAME=your_database_name
DATABASE_USER=your_database_user
DATABASE_PASSWORD=your_database_password
DATABASE_PORT=5432
DATABASE_SSL=false

# Google Cloud Configuration (choose one method)
# Method 1: Full service account JSON (single line, escaped)
GOOGLE_APPLICATION_CREDENTIALS_JSON={"type":"service_account","project_id":"..."}

# Method 2: Individual credentials (more secure)
GCP_PROJECT_ID=your_gcp_project_id
GCP_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\n...
GCP_CLIENT_EMAIL=your-service-account@project.iam.gserviceaccount.com

# Authentication
NEXTAUTH_SECRET=your_nextauth_secret_key
NEXTAUTH_URL=http://localhost:3000

# Security Dashboard Configuration
NEXT_PUBLIC_DASHBOARD_TITLE="Security Operations Center"
NEXT_PUBLIC_ENABLE_WEBSOCKETS=true
NEXT_PUBLIC_WEBSOCKET_URL=ws://localhost:8080

NEXT_PUBLIC_MAPBOX_TOKEN=mapbox_public_token

# Monitoring & Analytics
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id
SENTRY_DSN=your_sentry_dsn

# API Keys (for security monitoring integrations)
SECURITY_API_KEY=your_security_monitoring_api_key
LOG_AGGREGATION_API_KEY=your_log_aggregation_api_key

# Development
NEXT_PUBLIC_DEBUG_MODE=false
NEXT_PUBLIC_SHOW_DEBUG_INFO=false